<div class="loginbox">
	<h3>Login:</h3><br style="background-color:#33cc33;">
	<form id="login" name="login" autocomplete="off" method="post" action="/login">
		<div class="content">
			<label>Username: </label>
			<input type="text" name="username" autocomplete="on" required="required" autofocus="on" placeholder="CSU ID"></input>
			<label>Password: </label>
			<input type="password" name="password" required="required"></input>
			<br><br><button type="submit">Login</button>
			<br>
			<h3 style="color:aliceblue"><%=err%></h3>
		</div>
	</form>
</div>